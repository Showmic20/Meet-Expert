{
  "expo": {
    "name": "meet-expert",
    "slug": "meet-expert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/meetexpertlogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "meetexpert",
    "runtimeVersion": { "policy": "appVersion" },
    "splash": {
      "image": "./assets/images/meetexpertlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.yourname.meetexpert",
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourname.meetexpert",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/meetexpertlogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "extra": {
      "router": {},
      "eas": { "projectId": "a4a44bfc-b512-4af9-bb70-a5c24cd55e38" }
    },
    "owner": "swarup101",
    "experiments": { "tsconfigPaths": true }
  }
}
